{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "anhskohbo/no-captcha": "^2.3",
        "barryvdh/laravel-snappy": "^0.4.0",
        "bogardo/mailgun": "^5.0",
        "consoletvs/charts": "4.*",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~3.3",
        "geoip2/geoip2": "~2.0",
        "h4cc/wkhtmltoimage-amd64": "^0.12.3",
        "h4cc/wkhtmltoimage-i386": "^0.12.3",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "h4cc/wkhtmltopdf-i386": "^0.12.3",
        "intervention/image": "^2.4",
        "khill/lavacharts": "^3.1",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^4.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "^2.1",
        "pragmarx/countries": "^0.1.1",
        "pragmarx/tracker": "^3.2",
        "pusher/pusher-php-server": "^2.6",
        "spatie/laravel-google-calendar": "^1.0",
        "tymon/jwt-auth": "^0.5.12",
        "yajra/laravel-datatables-buttons": "^3.0",
        "yajra/laravel-datatables-oracle": "^8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
